        <solid
            android:color="#AA64D3F3" />
        <stroke
            android:width="0.25dp"
            android:color="#57C8E7" />
    </shape>
</item>
<item>
    <shape
        android:shape="rectangle">
        <solid android:color="@android:color/transparent" />
        <!--<stroke android:width="0.25dp" android:color="#D1D1D1" />-->
    </shape>
</item>
</selector>